{
  "name": "zell",
  "version": "2.0.0",
  "description": "Fully offline cross-platform file converter, compressor, editor, and compiler",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "build:web": "expo build:web",
    "build:wasm": "cd wasm-modules && npm run build",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run web\" \"wait-on http://localhost:19006 && electron .\"",
    "build:desktop": "expo export:web && electron .",
    "build:desktop:prod": "expo export:web && electron-builder",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "expo": "~49.0.0",
    "react": "18.3.1",
    "react-native": "0.72.6",
    "react-native-web": "~0.19.6",
    "react-dom": "18.3.1",
    "@expo/vector-icons": "^13.0.0",
    "expo-file-system": "~15.4.0",
    "expo-document-picker": "~11.5.0",
    "expo-sharing": "~11.5.0",
    "expo-status-bar": "~1.6.0",
    "expo-sqlite": "~11.3.0",
    "expo-constants": "~14.4.0",
    "expo-appearance": "~1.0.0",
    "expo-media-library": "~15.4.0",
    "expo-image-picker": "~14.3.0",
    "expo-av": "~13.4.0",
    "react-native-paper": "^5.10.6",
    "react-native-vector-icons": "^10.0.0",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "react-native-svg": "13.9.0",
    "react-native-canvas": "^0.1.38",
    "react-native-image-editor": "^2.3.0",
    "react-native-pdf": "^6.7.3",
    "react-native-fs": "^2.20.0",
    "react-native-zip-archive": "^6.0.15",
    "pdf-lib": "^1.17.1",
    "mammoth": "^1.6.0",
    "sharp": "^0.32.6",
    "jszip": "^3.10.1",
    "yauzl": "^2.10.0",
    "node-7z": "^2.1.2",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.72.2",
    "typescript": "^5.1.3",
    "jest": "^29.2.1",
    "eslint": "^8.45.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0"
  },
  "private": true
}
